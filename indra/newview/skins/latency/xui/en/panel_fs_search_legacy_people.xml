<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<panel
 name="panel_ls_people"
 label="People"
 left="1"
 height="566"
 width="780"
 follows="all"
 layout="topleft"
 border="false"
>
    <panel
     name="panel_ls_input"
     left="0"
     height="53"
     width="780"
     follows="top|left|right"
     layout="topleft"
     border="false"
    >
        <text
         name="search_text"
         top="12"
         top_pad="5"
         left="6"
         height="16"
         width="156"
         follows="left|top"
         layout="topleft"
         length="1"
         type="string"
        >
            Enter search terms:
        </text>
        <search_combo_box
         name="people_edit"
         top="29"
         left_delta="0"
         height="23"
         width="356"
         follows="left|top|right"
         layout="topleft"
        >
        </search_combo_box>
    </panel>
    <!-- Search Pane -->
    <panel
     name="panel_ls_scrolllist"
     top_pad="1"
     left="1"
     height="506"
     width="356"
     follows="all"
     layout="topleft"
     border="true"
    >
        <fs_scroll_list
         name="search_results_people"
         content_type="Agents"
         top="0"
         left="0"
         height="480"
         width="356"
         follows="all"
         layout="topleft"
         draw_heading="true"
        >
            <columns
             name="icon"
             label=""
             width="20"
            />
            <columns
             name="username"
             label="Name"
             relwidth="1"
            />
        </fs_scroll_list>
        <button
         name="people_back"
         label="Back"
         top_pad="2"
         left="3"
         height="23"
         width="100"
         follows="left|bottom"
         layout="topleft"
        />
        <button
         name="people_next"
         label="Next"
         left_pad="1"
         height="23"
         width="100"
         follows="left|bottom"
         layout="topleft"
        />
    </panel>
    <panel
     name="panel_profile_view"
     top_delta="-20"
     left="360"
     height="522"
     width="420"
     follows="top|right|bottom"
     layout="topleft"
	 class="panel_fs_profile"
    >
        <button
         name="people_profile_btn"
         top="0"
         left="-20"
         height="16"
         width="16"
         follows="right|top"
         layout="topleft"
		 image_overlay="tearoffbox.tga"
        />
		<view_border
		 follows="all"
		 top="20"
		 left="0"
		 width="420"
		 height="506"
		 layout="topleft"
         name="drop_target_rect_vis"
		/>
		<tab_container
		 follows="all"
		 top="24"
		 left="0"
		 width="420"
		 height="500"
		 halign="center"
		 layout="topleft"
		 name="panel_profile_tabs"
		 tab_position="top"
		 tab_min_width="55">
			<panel
			 class="panel_fs_profile_secondlife"
			 filename="panel_fs_profile_secondlife.xml"
			 label="2nd Life"
			 layout="topleft"
			 help_topic="profile_secondlife_tab"
			 name="panel_profile_secondlife" />
			<panel
			 class="panel_fs_profile_web"
			 filename="panel_fs_profile_web.xml"
			 label="Web"
			 layout="topleft"
			 help_topic="profile_web_tab"
			 name="panel_profile_web" />
			<panel
			 class="panel_fs_profile_interests"
			 filename="panel_fs_profile_interests.xml"
			 label="Interests"
			 layout="topleft"
			 help_topic="profile_interests_tab"
			 name="panel_profile_interests" />
			<panel
			 class="panel_fs_profile_picks"
			 filename="panel_fs_profile_picks.xml"
			 label="Picks"
			 layout="topleft"
			 help_topic="profile_picks_tab"
			 name="panel_profile_picks" />
			<panel
			 class="panel_fs_profile_classified"
			 filename="panel_fs_profile_classified.xml"
			 label="Classified"
			 layout="topleft"
			 help_topic="profile_classified_tab"
			 name="panel_profile_classified" />
			<panel
			 class="panel_fs_profile_firstlife"
			 filename="panel_fs_profile_firstlife.xml"
			 label="1st Life"
			 layout="topleft"
			 help_topic="profile_firstlife_tab"
			 name="panel_profile_firstlife" />
			<panel
			 class="panel_fs_profile_notes"
			 filename="panel_fs_profile_notes.xml"
			 label="Notes"
			 layout="topleft"
			 help_topic="profile_notes_tab"
			 name="panel_profile_notes" />
		</tab_container>
    </panel>
</panel>
