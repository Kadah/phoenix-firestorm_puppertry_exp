<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<toolbar_view
 name="toolbar view"
 top="0"
 left="0"
 height="500"
 width="1024"
 follows="all"
 layout="topleft"
 mouse_opaque="false"
 tab_stop="false"
 visible="true"
>
	<layout_stack
	 name="bottom_toolbar_stack"
	 top="0"
	 left="0"
	 height="500"
	 width="1024"
	 follows="all"
	 border_size="0"
	 mouse_opaque="false"
	 orientation="vertical"
	>
		<layout_panel
		 name="vertical_toolbar_panel"
		 height="500"
		 width="1024"
		 auto_resize="true"
		 mouse_opaque="false"
		 user_resize="false"
		>
			<layout_stack
			 name="vertical_toolbar_stack"
			 top="0"
			 left="0"
			 height="500"
			 width="1024"
			 follows="all"
			 mouse_opaque="false"
			 orientation="horizontal"
			>
				<layout_panel
				 name="left_toolbar_panel"
				 height="500"
				 width="30"
				 auto_resize="false"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<toolbar
					 name="toolbar_left"
					 top="0"
					 bottom="-1"
					 left="0"
					 width="30"
					 follows="left|top|bottom"
					 button_display_mode="icons_only"
					 button_panel.bg_opaque_image="Rounded_Rect_Right"
					 side="left"
					>
						<icon
						 name="caret"
						 top="20"
						 left="10"
						 height="15"
						 width="28"
						 follows="left|top"
						 layout="topleft"
						 image_name="Caret_Left_Icon"
						 tab_stop="false"
						 visible="false"
						/>
					</toolbar>
				</layout_panel>
				<layout_panel
				 name="non_toolbar_panel"
				 height="100"
				 width="200"
				 auto_resize="true"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<view
					 name="floater_snap_region"
					 top="0"
					 left="0"
					 height="100"
					 width="200"
					 follows="all"
					 mouse_opaque="false"
					/>
					<view
					 name="minimized_floaters_region"
					 top="0"
					 left="0"
					 height="100"
					 width="160"
					 follows="left|top|bottom"
					 mouse_opaque="false"
					/>
					<panel
					 name="chiclet_container"
					 top="40"
					 left_pad="10"
					 height="30"
					 width="20"
					 follows="left|top|right"
					 layout="topleft"
					 mouse_opaque="false"
					 tab_stop="false"
					>
					</panel>
				</layout_panel>
				<layout_panel
				 name="right_toolbar_panel"
				 height="500"
				 width="30"
				 auto_resize="false"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<toolbar
					 name="toolbar_right"
					 top="0"
					 bottom="-1"
					 left="0"
					 width="30"
					 follows="right|top|bottom"
					 button_display_mode="icons_only"
					 button_panel.bg_opaque_image="Rounded_Rect_Left"
					 side="right"
					>
						<icon
						 name="caret"
						 top="20"
						 left="10"
						 height="15"
						 width="28"
						 follows="left|top"
						 layout="topleft"
						 image_name="Caret_Right_Icon"
						 tab_stop="false"
						 visible="false"
						/>
					</toolbar>
				</layout_panel>
			</layout_stack>
		</layout_panel>
		<!-- Nearby chat panel, resting on top of the bottom toolbar -->
		<layout_panel
		 name="bottom_chat_bar_panel"
		 height="46"
		 width="1024"
		 auto_resize="false"
		 mouse_opaque="false"
		 user_resize="false"
		>
			<!-- Horizontal layout stack to divide chat bar and utility bar on the right -->
			<layout_stack
			 name="chat_bar_utility_bar_stack"
			 top="0"
			 left="0"
			 height="46"
			 width="1024"
			 follows="all"
			 border_size="0"
			 mouse_opaque="false"
			 orientation="horizontal"
			>
				<!-- Chat panel -->
				<layout_panel
				 name="chat_panel"
				 height="46"
				 width="844"
				 auto_resize="true"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<!-- Layout stack to make showing and hiding easy -->
					<layout_stack
					 name="bottom_chat_stack"
					 top="0"
					 left="0"
					 height="46"
					 width="844"
					 follows="all"
					 border_size="0"
					 mouse_opaque="false"
					 orientation="vertical"
					>
						<!-- Stand Up/Flycam panel, resting on top of the chat bar
	   Also takes up the space when chat bar panel gets hidden -->
						<layout_panel
						 name="stand_stop_flying_layout_panel"
						 height="26"
						 width="844"
						 auto_resize="true"
						 chrome="true"
						 mouse_opaque="false"
						 user_resize="false"
						>
							<panel
							 name="state_management_buttons_container"
							 top="4"
							 left="350"
							 height="20"
							 width="158"
							 follows="bottom|left"
							 mouse_opaque="false"
							 tab_stop="false"
							 visible="true"
							/>
						</layout_panel>
						<!-- Stand Up/Flycam panel, resting on top of the chat bar
	   Also takes up the space when chat bar panel gets hidden -->
						<!-- Panel to house the chat bar in, will be set visible/invisible in code
	   as parent of the chat control -->
						<layout_panel
						 name="bottom_chat_bar_panel"
						 height="26"
						 width="844"
						 auto_resize="false"
						 background_opaque="true"
						 background_visible="true"
						 bg_opaque_image="Rounded_Rect_Top_2"
						 bg_opaque_image_overlay="DkGray"
						 mouse_opaque="false"
						 user_resize="false"
						>
							<button
							 name="local_chat_button"
							 label="Nearby Chat"
							 tool_tip="Shows/hides nearby chat window (CTRL+H)"
							 top="4"
							 left="5"
							 height="20"
							 width="80"
							 control_name="floater_vis_nearby_chat"
							>
								<commit_callback
								 function="Floater.Toggle"
								 parameter="fs_nearby_chat"
								/>
							</button>
							<fs_nearby_chat_control
							 name="default_chat_bar"
							 default="true"
							 label="To nearby chat"
							 left_pad="8"
							 right="-4"
							 height="20"
							 follows="all"
							 layout="topleft"
							 spellcheck="true"
							/>
						</layout_panel>
						<!-- Panel to house the chat bar in, will be set visible/invisible in code
	   as parent of the chat control -->
					</layout_stack>
					<!-- Layout stack to make showing and hiding easy -->
				</layout_panel>
				<!-- Chat panel -->
				<!-- Utility Bar layout panel -->
				<layout_panel
				 name="utility_bar_layout_panel"
				 height="46"
				 width="229"
				 follows="bottom|left"
				 layout="topleft"
				 auto_resize="false"
				 chrome="true"
				 invisibility_control="MouseLookEnabled"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<!-- Utility Bar panel -->
					<panel
					 name="utility_bar_panel"
					 top="20"
					 left="2"
					 height="26"
					 width="225"
					 follows="bottom|left"
					 layout="topleft"
					 background_opaque="true"
					 background_visible="true"
					 bg_opaque_image="Rounded_Rect_Top_2"
					 bg_opaque_image_overlay="DkGray"
					>
						<!-- Parcel Stream controls -->
						<icon
						 name="parcel_stream_icon"
						 top="4"
						 left="3"
						 height="20"
						 width="16"
						 follows="bottom|left"
						 layout="topleft"
						 image_name="icn_label_music.tga"
						/>
						<button
						 name="utility_parcel_audio_stream_button"
						 label=""
						 tool_tip="Play/Pause parcel audio stream"
						 left_pad="2"
						 height="20"
						 width="40"
						 follows="bottom|left"
						 layout="topleft"
						 image_overlay="icn_play.tga"
						 is_toggle="true"
						>
							<!-- Done in code for now -Zi
		  enabled_control="ToggleParcelStreamEnabled"
		  control_name="ParcelStreamRunning"
  <button.commit_callback
		  function="Parcel.Streasm.Clicked"
		  parameter="play_pause" />
-->
						</button>
						<!-- Parcel Stream controls -->
						<!-- Parcel Media controls -->
						<icon
						 name="parcel_media_icon"
						 left_pad="5"
						 height="20"
						 width="16"
						 follows="bottom|left"
						 layout="topleft"
						 image_name="icn_label_media.tga"
						/>
						<button
						 name="utility_parcel_media_button"
						 label=""
						 tool_tip="Play/Pause parcel media"
						 top="4"
						 left_pad="3"
						 height="20"
						 width="40"
						 follows="bottom|left"
						 layout="topleft"
						 image_overlay="icn_play.tga"
						 is_toggle="true"
						>
							<!-- Done in code for now -Zi
		  enabled_control="ToggleParcelMediaEnabled"
		  control_name="ParcelMediaRunning"
  <button.commit_callback
		  function="Parcel.Media.Clicked"
		  parameter="play_pause" />
-->
						</button>
						<!-- Parcel Media controls -->
						<!-- Volume controls -->
						<slider
						 name="System Volume"
						 tool_tip="Adjust system volume"
						 left_pad="3"
						 height="20"
						 width="43"
						 follows="bottom|left"
						 layout="topleft"
						 control_name="AudioLevelMaster"
						 increment="0.025"
						 initial_value="0.5"
						 show_text="false"
						>
							<!-- Copied from status bar, but doesn't seem to work:
	   "No callback found for: 'Pref.setControlFalse' in control: System Volume"
  <slider.commit_callback
		  function="Pref.setControlFalse"
		  parameter="MuteAudio" /> -->
						</slider>
						<button
						 name="mute_audio"
						 label=""
						 tool_tip="Mute system audio"
						 top="4"
						 left_pad="4"
						 height="20"
						 width="20"
						 follows="bottom|left"
						 layout="topleft"
						 control_name="MuteAudio"
						 image_selected="AudioMute_Off"
						 image_unselected="Audio_Off"
						 is_toggle="true"
						>
						</button>
						<button
						 name="show_volume_controls_button"
						 tool_tip="Show volume controls"
						 height="20"
						 width="22"
						 left_pad="0"
						 layout="topleft"
						 follows="bottom|left"
						 is_toggle="true"
						 image_unselected="PushButton_Up"
						 image_selected="PushButton_Down">
						 <button.commit_callback
							 function="Floater.ToggleOrBringToFront"
							 parameter="fs_volume_controls" />
						</button>
						<!-- Volume controls -->
					</panel>
					<!-- Utility Bar panel -->
				</layout_panel>
				<!-- Utility Bar layout panel -->
				<!-- Voice Chat layout panel -->
				<layout_panel
				 name="voice_chat_controls_layout_panel"
				 height="46"
				 width="133"
				 layout="topleft"
				 auto_resize="false"
				 chrome="true"
				 invisibility_control="MouseLookEnabled"
				 mouse_opaque="false"
				 user_resize="false"
				 visibility_control="EnableVoiceChat"
				>
					<!-- Voice Chat panel -->
					<panel
					 name="voice_chat_controls_panel"
					 top="20"
					 left="1"
					 height="26"
					 width="130"
					 follows="bottom|left"
					 layout="topleft"
					 background_opaque="true"
					 background_visible="true"
					 bg_opaque_image="Rounded_Rect_Top_2"
					 bg_opaque_image_overlay="DkGray"
					>
						<button
						 name="show_voice_controls_button"
						 label=""
						 tool_tip="Shows the nearby voice speakers and controls"
						 left="2"
						 height="20"
						 width="38"
						 follows="bottom|left"
						 layout="topleft"
						 image_overlay="active_speakers.tga"
						>
							<button.commit_callback
							 function="Floater.Toggle"
							 parameter="fs_voice_controls"
							/>
						</button>
						<button
						 name="utility_talk_button"
						 label="Talk"
						 tool_tip="Press this button to talk on voice. Click the lock to toggle Push-to-Talk mode on and off."
						 left_pad="2"
						 height="20"
						 width="56"
						 follows="bottom|left"
						 layout="topleft"
						 pad_left="10"
						>
							<!-- Done in code for now -Zi
  <mouse_down_callback
		   function="Agent.PressMicrophone"
		   parameter="speak" />
  <mouse_up_callback
		   function="Agent.ReleaseMicrophone"
		   parameter="speak" />

		   is_running_function="Agent.IsMicrophoneOn"
		   is_running_parameters="speak"
-->
						</button>
						<button
						 name="utility_push_to_talk_lock_button"
						 label=""
						 tool_tip="Toggles push-to-talk function"
						 top_delta="2"
						 left_delta="4"
						 height="16"
						 width="16"
						 follows="bottom|left"
						 layout="topleft"
						 control_name="PushToTalkToggle"
						 image_selected="ptt_lock_on.tga"
						 image_unselected="ptt_lock_off.tga"
						 is_toggle="true"
						>
						</button>
						<nearby_voice_monitor
						 name="nearby_voice_output_monitor"
						 tool_tip="Current voice channel activity"
						 label=""
						 top="2"
						 left_pad="38"
						 height="20"
						 width="35"
						 follows="top|right"
						 layout="topleft"
						 auto_update="true"
						 draw_border="false"
						/>
						<!-- Do we need this button? -Zi -->
						<!--
  <button name="utility_show_voice_channel_button"
		  tool_tip="(TODO) Shows current voice channel"
		  label=""
		  image_overlay="arrow_up.tga"
		  height="20"
		  top="4"
		  left_pad="46"
		  layout="topleft"
		  follows="bottom|left"
		  enabled="false"
		  width="20">
  </button> -->
					</panel>
					<!-- Voice Chat panel -->
				</layout_panel>
				<!-- Voice Chat layout panel -->
				<!-- Quick Preferences Button layout panel -->
				<layout_panel
				 name="quick_preferences_button_layout_panel"
				 height="66"
				 width="96"
				 layout="topleft"
				 auto_resize="false"
				 chrome="true"
				 invisibility_control="MouseLookEnabled"
				 mouse_opaque="false"
				 user_resize="false"
				>
					<!-- Quick Preferences Button panel -->
					<panel
					 name="quick_preferences_button_panel"
					 top="40"
					 left="1"
					 height="26"
					 width="93"
					 follows="bottom|left"
					 layout="topleft"
					 background_opaque="true"
					 background_visible="true"
					 bg_opaque_image="Rounded_Rect_Top_2"
					 bg_opaque_image_overlay="DkGray"
					>
						<button
						 name="enable_disable_ao_button"
						 label=""
						 tool_tip="Enable/Disable client animation overrider"
						 top="4"
						 left="4"
						 height="20"
						 width="40"
						 follows="bottom|left"
						 layout="topleft"
						 control_name="UseAO"
						 image_disabled="SplitButtonLeft_Disabled"
						 image_overlay="ao_toggle_18"
						 image_selected="SplitButtonLeft_Selected"
						 image_unselected="SplitButtonLeft_Off"
						 is_toggle="true"
						>
						</button>
						<button
						 name="show_ao_interface_button"
						 label=""
						 tool_tip="Show/Hide client animation overrider interface"
						 left_pad="0"
						 height="20"
						 width="20"
						 follows="bottom|left"
						 layout="topleft"
						 image_disabled="SplitButtonRight_Disabled"
						 image_overlay="arrow_up.tga"
						 image_selected="SplitButtonRight_Selected"
						 image_unselected="SplitButtonRight_Off"
						>
							<button.commit_callback
							 function="Floater.Toggle"
							 parameter="animation_overrider"
							/>
						</button>
						<!-- Tooltip text defined in commands.xml - buttons can't use tool_tip_ref yet -->
						<button
						 name="show_quick_prefs_button"
						 label=""
						 tool_tip="Quick preferences for changing often used settings"
						 top="4"
						 left_pad="3"
						 height="20"
						 width="22"
						 follows="bottom|left"
						 layout="topleft"
						 image_overlay="phoenix_18"
						>
							<button.commit_callback
							 function="Floater.Toggle"
							 parameter="quickprefs"
							/>
						</button>
					</panel>
					<!-- Quick Preferences Button panel -->
				</layout_panel>
				<!-- Quick Preferences Button layout panel -->
			</layout_stack>
			<!-- Horizontal layout stack to divide chat bar and utility bar on the right -->
		</layout_panel>
		<!-- Nearby chat panel, resting on top of the bottom toolbar -->
		<layout_panel
		 name="bottom_toolbar_panel"
		 height="28"
		 width="1024"
		 auto_resize="false"
		 background_opaque="true"
		 bg_opaque_color="DkGray"
		 bg_visible="true"
		 mouse_opaque="false"
		 user_resize="false"
		>
			<button
			 name="open_chat_bar_button"
			 label=""
			 tool_tip="Show chat bar (Enter)"
			 top="2"
			 left="5"
			 height="20"
			 width="60"
			 follows="left|top"
			 layout="topleft"
			 chrome="true"
			 control_name="MainChatbarVisible"
			 image_overlay="nearbychat_18"
			 is_toggle="true"
			/>
			<toolbar
			 name="toolbar_bottom"
			 top="0"
			 left_pad="2"
			 right="-1"
			 height="28"
			 follows="left|right|bottom"
			 layout="topleft"
			 button_display_mode="text_only"
			 layout_style="fill"
			 side="bottom"
			 visible="true"
			>
				<icon
				 name="caret"
				 top="20"
				 left="10"
				 height="24"
				 width="15"
				 follows="left|top"
				 layout="topleft"
				 image_name="Caret_Bottom_Icon"
				 tab_stop="false"
				 use_draw_context_alpha="false"
				 visible="false"
				/>
			</toolbar>
		</layout_panel>
	</layout_stack>
    
    <!-- FS:Ansariel: For chiclets and notices at bottom -->
    <panel
     name="chiclet_container_bottom"
     bottom="-60"
     right="-20"
     height="30"
     width="20"
     follows="left|bottom|right"
     layout="topleft"
     bg_visible="false"
     mouse_opaque="false"
     tab_stop="false"
    >
    </panel>
    <!-- END FS:Ansariel: For chiclets and notices at bottom -->
</toolbar_view>
