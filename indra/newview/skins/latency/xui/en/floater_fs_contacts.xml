<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<floater
 can_close="true"
 can_drag_on_left="false"
 can_minimize="false"
 can_resize="true"
 can_tear_off="true"
 height="390"
 layout="topleft"
 min_height="240"
 min_width="260"
 name="imcontacts"
 save_rect="true"
 save_visibility="true"
 single_instance="true"
 title="Contacts"
 width="396">
 	<string
	 name="no_friends"
	 value="No friends" />
    <string
     name="no_filtered_groups_msg"
     value="No groups containing &quot;[SEARCH_TERM]&quot; found." />
	<string
     name="no_groups_msg"
     value="Looking for Groups to join? Try [secondlife:///app/search/groups Search]." />
    <tab_container
     follows="all"
     height="390"
     layout="topleft"
     left="0"
     name="friends_and_groups"
     tab_position="top"
     tab_width="80"
     tab_max_width = "80"
     tab_height="20"
     use_custom_icon_ctrl="false"
     tab_icon_ctrl_pad="2"
     halign="left"
     use_ellipses="true"
     top="0"
     width="393">
      <first_tab
       tab_bottom_image_flash="Toolbar_Left_Flash"/>
      <middle_tab
       tab_bottom_image_flash="Toolbar_Middle_Flash"/>
      <last_tab
       tab_bottom_image_flash="Toolbar_Right_Flash"/>
      
	  <panel
	   bottom="0"
	   filename="panel_fs_contacts_friends.xml"
	   label="Friends"
	   left="0"
	   right="-1"
	   name="friends_panel" />
	  
      <panel
       bottom="0"
	   filename="panel_fs_contacts_groups.xml"
       label="Groups"
       left="0"
	   right="-1"
       name="groups_panel"/>

	  <panel
	   bottom="0"
	   filename="panel_fs_contacts_sets.xml"
	   class="contact_sets_panel"
	   label="Contact Sets"
	   left="0"
	   right="-1"
	   name="contact_sets_panel" />
    </tab_container>
</floater>
