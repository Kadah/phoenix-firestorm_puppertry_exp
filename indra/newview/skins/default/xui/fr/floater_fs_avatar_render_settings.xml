<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<floater name="avatarrendersettings" title="Exceptions à l'affichage de l'avatar">
	<floater.string name="av_render_never">
		Jamais
	</floater.string>
	<floater.string name="av_render_always">
		Toujours
	</floater.string>
	<filter_editor label="Filtrer les avatars" name="filter_input" />
	<menu_button name="plus_btn" tool_tip="Ajouter une nouvelle personne à la liste" />
	<name_list name="avatar_list">
		<name_list.columns label="Nom" name="name" />
		<name_list.columns label="Tous les détails" name="render_setting" />
	</name_list>
	<button name="close_btn" label="Fermer" />
</floater>
